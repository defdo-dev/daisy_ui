<div class="flex flex-col px-2">
  <.breadcrumbs
    :let={entry}
    entries={[%{text: "blog", to: "#"}, %{text: "post", to: "#"}, %{text: "welcome", to: "#"}]}
  >
    <%= if entry.is_last do %>
      <span class="capitalize"><%= entry.content.text %></span>
    <% else %>
      <.link href={entry.content.to}>
        <span class="capitalize"><%= entry.content.text %></span>
      </.link>
    <% end %>
  </.breadcrumbs>

  <.breadcrumbs :let={entry} entries={["home", "documents", "add document"]}>
    <%= if entry.is_last do %>
      <svg
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        class="w-4 h-4 mr-2 stroke-current"
      >
        <path
          stroke-linecap="round"
          stroke-linejoin="round"
          stroke-width="2"
          d="M9 13h6m-3-3v6m5 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"
        >
        </path>
      </svg>
      <span class="capitalize"><%= entry.content %></span>
    <% else %>
      <.link href="#">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 24 24"
          class="w-4 h-4 mr-2 stroke-current"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M3 7v10a2 2 0 002 2h14a2 2 0 002-2V9a2 2 0 00-2-2h-6l-2-2H5a2 2 0 00-2 2z"
          >
          </path>
        </svg>
        <span class="capitalize"><%= entry.content %></span>
      </.link>
    <% end %>
  </.breadcrumbs>

  <.link href="https://shop.defdo.dev" target="_blank">
    Shop
  </.link>

  <.link navigate="/">
    Redirect
  </.link>

  <.link patch="/">
    Patch
  </.link>

  <.menu
    :let={{key, value}}
    entries={["Category 1": ["Item 1", "Item 2"], "Category 2": ["Item 3", "Item 4"]]}
    class="w-56 p-2 border rounded-box border-primary"
  >
    <%= if is_list(value) do %>
      <.menu_item class="menu-title">
        <span><%= key %></span>
      </.menu_item>
      <%= for item <- value do %>
        <.menu_item>
          <.link>
            <%= item %>
          </.link>
        </.menu_item>
      <% end %>
    <% end %>
  </.menu>
