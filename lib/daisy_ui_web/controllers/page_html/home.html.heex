<div class="bg-base-200 px-1 py-2 rounded-md shadow-md">
  <div class="flex flex-col px-4">
    <div class="flex items-center">
      <Heroicons.computer_desktop solid class="w-4 h-4 stroke-current inline hover:text-accent" />
      <span class="text-accent ml-2">Simple Snipped (wip)</span>
    </div>
    <div class="relative">
      <div class="absolute right-4 mt-2 z-10">
        <button class="hover:text-accent">
          <Heroicons.document_duplicate solid class="w-4 h-4 stroke-current inline" />
        </button>
      </div>
      <.code class="font-mono whitespace-pre">
        [%{
        id: 1,
        name: "Jon",
        job: "Quality Control Specialist",
        fav_color: "Blue"
        },
        %{
        id: 2
        name: "Paul",
        job: "Salesman",
        fav_color: "Red"
        }]
      </.code>
    </div>
  </div>
</div>
