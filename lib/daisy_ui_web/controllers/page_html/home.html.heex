<.steps
  :let={item}
  orientation={:vertical}
  entries={["Register", "Choose Plan", "Purchase", "Receive Products"]}
>
  <.step class="step-primary">
    <%= item %>
  </.step>
</.steps>

<.drawer drawer_id="my-drawer" class_content="grid grid-cols-2">
  <:drawer_content>
    <div class="">
      <.steps :let={item} entries={["Register", "Choose Plan", "Purchase", "Receive Product"]}>
        <.step class="step-info">
          <%= item %>
        </.step>
      </.steps>
    </div>

    <div class="justify-self-end">
      <label for="my-drawer" class="btn btn-primary drawer-button">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 24 24"
          class="inline-block w-6 h-6 stroke-current"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M4 6h16M4 12h16M4 18h16"
          >
          </path>
        </svg>
      </label>
    </div>
  </:drawer_content>

  <:drawer_sidebar>
    <.menu
      :let={{key, value}}
      to="#"
      entries={["Category 1": ["Item 1", "Item 2"], "Category 2": ["Item 3", "Item 4"]]}
      class="w-56 p-2 bg-neutral text-neutral-content"
    >
      <%= if is_list(value) do %>
        <.menu_item class="menu-title">
          <span><%= key %></span>
        </.menu_item>
        <%= for item <- value do %>
          <.menu_item>
            <.link>
              <%= item %>
            </.link>
          </.menu_item>
        <% end %>
      <% end %>
    </.menu>
  </:drawer_sidebar>
</.drawer>
