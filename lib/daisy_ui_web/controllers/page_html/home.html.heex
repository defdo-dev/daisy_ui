<div class="bg-base-200 px-4 py-8 rounded-md shadow-md overflow-x-auto">
  <.form_control>
    <.label_control>
      <.label_text>What is your name?</.label_text>
      <.label_text_alt>defdo</.label_text_alt>
    </.label_control>
    <.input
      id="0"
      value=""
      name="phone_number"
      placeholder="Phone number"
      class="w-full"
      autocomplete="off"
    />
    <.label_control>
      <.label_text_alt>Type the msisdn</.label_text_alt>
      <.label_text_alt>help?</.label_text_alt>
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input id="1" type="file" name="file-upload" value="" class="w-full file-input-success" />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control class="cursor-pointer">
      <.checkbox
        id="2"
        value=""
        name="checkbox-0"
        class="checkbox-accent"
        label="Send me, promotions"
      />
      <.input
        id="2"
        value="true"
        name="checkbox-1"
        type="checkbox"
        class="checkbox-primary"
        label="I'm agree with the Terms and Condition"
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input id="6" value="" name="radio-0" type="radio" class="radio-primary" label="Card" />
      <.input id="5" value="" name="radio-0" type="radio" class="radio-primary" label="Store" />
      <.input id="4" value="" name="radio-0" type="radio" class="radio-primary" />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input
        id="0"
        type="range"
        name="range-0"
        min="0"
        max="100"
        value="35"
        class="w-full range-primary"
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input
        id="1"
        name="w"
        min="0"
        max="100"
        value="35"
        type="range"
        step="35"
        class="w-full"
        range_steps={5}
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input type="rating" name="foo" id="love" value="3.0" />
      <.rating
        id="lvou"
        name="foo2"
        rating_half={true}
        value="2.5"
        rating_class="bg-accent mask-star-2"
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input
        id="1"
        type="select"
        name="favorite_character"
        value=""
        prompt="Pick your favorite Simpson"
        class="w-full select-bordered"
        options={[Main: ~w(Homer Marge Bart Lisa Maggie), Extras: ~w(Barney Apu Krusty)]}
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input
        id="1"
        type="select"
        name="favorite_lang"
        value=""
        prompt="Pick your favorite code language"
        class="w-full select-accent"
        options={["Elixir": "elixir", Rust: "rust", Go: "go", Python: "python", Javascript: "js"]}
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.input
        id="1"
        name="no-redacted"
        type="textarea"
        class="textarea-accent"
        value={nil}
        placeholder="Type your text!"
      />
    </.label_control>
  </.form_control>

  <.form_control>
    <.label_control>
      <.toggle name="toggle_1" id="1" value={nil} />
      <.toggle
        name="toggle_1"
        id="1"
        value={true}
        class="toggle-md toggle-secondary"
        label="Dark Theme"
      />
    </.label_control>
  </.form_control>

  <.table
    id="10"
    class="w-full"
    rows={[
      %{id: 1, name: "Cy Ganderton", job: "Quality Control Specialist", fav_color: "Blue"},
      %{id: 1, name: "José", job: "Salesman", fav_color: "Red"}
    ]}
  >
    <:col :let={user} label="ID"><%= Map.get(user, :id, "") %></:col>
    <:col :let={user} label="Name"><%= user.name %></:col>
    <:col :let={user} label="Job"><%= user.job %></:col>
    <:col :let={user} label="Favourite Color"><%= user.fav_color %></:col>

    <:action>
      <span>
        <Heroicons.trash
          solid
          class="w-3 h-3 stroke-current inline hover:text-accent hover:pointer-hand"
        />
      </span>
    </:action>
  </.table>
</div>
